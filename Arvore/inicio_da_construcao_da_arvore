
#include <stdlib.h>
#include <stdio.h>

typedef struct No{
    int dado;
    struct No *esq,*dir;
}No;

No* CriarNo(int num){
    No* novo=(No*)malloc(sizeof(No));
    if(!novo){
        printf("Erro ao criar NÃ³\n");
        exit(1);
    }
    novo->dado=num;
    novo->esq=NULL;
    novo->dir=NULL;
    return novo;
}
void inserir(No **raiz,int num){
   if(*raiz==NULL){
       *raiz=CriarNo(num);
       return;
   } 
    if(num<(*raiz)->dado){
        inserir(&((*raiz)->esq),num);
    }
    else if(num>(*raiz)->dado){
        inserir(&((*raiz)->dir),num);
    }
}
void em_ordem(No*raiz){
    if(!raiz) return;
    em_ordem(raiz->esq);
    printf("%d ",raiz->dado);
    em_ordem(raiz->dir);
    
}
int main()
{
    No *raiz=NULL;
    inserir(&raiz,10);
    inserir(&raiz,15);
    inserir(&raiz,5);
    em_ordem(raiz);

    return 0;
}
