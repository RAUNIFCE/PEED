

#include <stdio.h>
#include <stdbool.h>
#define N 5
int fila[N];
int p=0;
int u=0;
int size=0;
bool fila_vazia(){
    //return p==u;
    return size==0;
}
bool fila_cheia(){
    //return (u+1) % N==p;
    return size==u;
}
void enqueue(int valor){
    if(fila_cheia()){
        printf("Erro: fila cheia!\n");
        return false;
    }
    fila[u]=valor;
    u=(u+1) % N; 
    size++;
    return true;
}
bool dequeue(){
    if(fila_vazia()){
        printf("Erro: fila vazia!\n");
        return false;
    }
    int x=fila[p];
    p=(p+1) % N;
    printf("Removido: %d \n",fila[p-1]);
    size--;
    return true;
}
void mostrar_fila(){
  if(fila_vazia()){
        printf("Erro: fila vazia!\n");
        return;
    }   
    printf("fila: ");
    for(int i=p;i!=u;i=(i+1)%N){
        printf("%d ",fila[i]);
        
    }
    printf("\n");
}
int main()
{
    enqueue(10);
    enqueue(20);
    enqueue(30);
    mostrar_fila();
    dequeue();
    dequeue();
    dequeue();
    
    mostrar_fila();
    enqueue(40);
    enqueue(50);
    enqueue(60);
    enqueue(60);
    enqueue(60);
    mostrar_fila();
    
}
