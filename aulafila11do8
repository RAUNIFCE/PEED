

#include <stdio.h>
#include <stdbool.h>
#define N 5
int fila[N];
int p=0;
int u=0;
int size=0;
bool fila_vazia(){
    //return p==u;
    return size==0;
}
bool fila_cheia(){
    //return (u+1) % N==p;
    return size==N;
}
void enqueue(int valor){
    if(fila_cheia()){
        printf("Erro: fila cheia!\n");
        return false;
    }
    fila[u]=valor;
    u=(u+1) % N; 
    size++;
    return true;
}
bool dequeue(){
    if(fila_vazia()){
        printf("Erro: fila vazia!\n");
        return false;
    }
    int x=fila[p];
    p=(p+1) % N;
    printf("Removido: %d \n",fila[p-1]);
    size--;
    return true;
}
void mostrar_fila(){
     
    printf("fila: ");
    int count=0;
    int i=p;
    while(count<size){
        printf("%d ",fila[i]);
        i=(i+1)%N;
        count++;
    }
    printf("\n");
}
int main()
{
    enqueue(10);
    enqueue(20);
    enqueue(30);
    enqueue(40);
    mostrar_fila();
    dequeue();
    enqueue(50);
    dequeue();
    dequeue();
    dequeue();
    
    mostrar_fila();
    enqueue(100);
    enqueue(110);
    enqueue(120);
    enqueue(130);
    enqueue(140);
    mostrar_fila();
    
}
